{
	"name": "pyutils",
	"displayName": "PyUtils",
	"description": "PyUtils",
	"publisher": "beamy",
	"version": "0.0.1",
	"repository": {
		"url": "url.to.repository.tld"
	},
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"workspaceContains:**/.pyutils/deactivate-me",
		"workspaceContains:.pyutils/deactivate-me"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "beamy-container",
					"title": "PyUtils",
					"icon": "$(wrench)"
				}
			],
			"panel": [
				{
					"id": "beamy-panel",
					"title": "Panel Test",
					"icon": "$(wrench)"
				}
			]
		},
		"views": {
			"beamy-panel": [
				{
					"status": "status",
					"name": "Status",
					"icon": "$(debug-step-over)"
				}
			],
			"beamy-container": [
				{
					"id": "polyglot",
					"name": "Polyglot",
					"icon": "$(library)",
					"contextualTitle": "Polyglot"
				},
				{
					"id": "translator",
					"name": "Translator",
					"icon": "$()",
					"contextualTitle": "Translator"
				},
				{
					"id": "notes",
					"name": "Notes",
					"icon": "$(pencil)",
					"contextualTitle": "Notes"
				},
				{
					"id": "clipboard",
					"name": "Clipboard",
					"icon": "$(scissors)",
					"contextualTitle": "Clipboard"
				},
				{
					"id": "scratchpad",
					"name": "Scratchpad",
					"icon": "$(pencil)",
					"contextualTitle": "Notes"
				},
				{
					"id": "locakdoc",
					"name": "Local Documentation",
					"icon": "media/dep.svg",
					"contextualTitle": "Local Documentation"
				},
				{
					"id": "unicode",
					"name": "Unicode Browser",
					"icon": "$(symbol-array)",
					"contextualTitle": "unicode"
				},
				{
					"id": "system",
					"name": "System",
					"icon": "$(paintcan)",
					"contextualTitle": "systemd"
				},
				{
					"id": "Utils",
					"name": "Utils",
					"icon": "$(vm)",
					"contextualTitle": "Utils"
				},
				{
					"id": "noise",
					"name": "Noise",
					"icon": "$(notebook-render-output)",
					"contextualTitle": "noisesynth"
				},
				{
					"id": "draw",
					"name": "Draw",
					"icon": "$(paintcan)",
					"contextualTitle": "svgdraw"
				},
				{
					"id": "linguist",
					"name": "Linguist",
					"icon": "media/dep.svg",
					"contextualTitle": "Linguist"
				},
				{
					"id": "wave",
					"name": "Wave",
					"icon": "media/dep.svg",
					"contextualTitle": "Wave"
				}
			]
		},
		"customEditors": [
			{
				"viewType": "pyutils.polyglot",
				"displayName": "Polyglot",
				"selector": [
					{
						"filenamePattern": "*.pg"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.translator",
				"displayName": "Translator",
				"selector": [
					{
						"filenamePattern": "*.pg"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.notes",
				"displayName": "Notes",
				"selector": [
					{
						"filenamePattern": "*.md"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.clipboard",
				"displayName": "Clipboard",
				"selector": [
					{
						"filenamePattern": "*.cb"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.scratchpad",
				"displayName": "Scrachpad",
				"selector": [
					{
						"filenamePattern": "*.pg"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.doc",
				"displayName": "Documentation",
				"selector": [
					{
						"filenamePattern": "*.dc"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.unicode",
				"displayName": "Unicode",
				"selector": [
					{
						"filenamePattern": "*.uc"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.linguist",
				"displayName": "Linguist",
				"selector": [
					{
						"filenamePattern": "*.lg"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.draw",
				"displayName": "Draw",
				"selector": [
					{
						"filenamePattern": "*.dr"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.wave",
				"displayName": "Wave",
				"selector": [
					{
						"filenamePattern": "*.wv"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "pyutils.noise",
				"displayName": "Noise",
				"selector": [
					{
						"filenamePattern": "*.ns"
					}
				],
				"priority": "option"
			}
		],
		"themes": [
			{
				"label": "Viradora",
				"uiTheme": "vs-dark",
				"path": "./themes/viradora-color-theme.json"
			}
		],
		"colors": [
			{
				"id": "superstatus.error",
				"description": "Color for error message in the status bar.",
				"defaults": {
					"dark": "errorForeground",
					"light": "errorForeground",
					"highContrast": "#010203",
					"highContrastLight": "#feedc3"
				}
			}
		],
		"keybindings": [
			{
				"command": "run",
				"key": "ctrl+alt+r"
			},
			{
				"command": "pyutils.build",
				"key": "ctrl+b"
			}
		],
		"menus": {
			"editor/title/run": [
				{
					"command": "beamy.pyutils.run",
					"group": "navigation"
				}
			],
			"editor/title/context": [
				{
					"command": "beamy.pyutils.title_command ",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"command": "beamy.pyutils.debug",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"title": "PyUtils",
			"properties": {
				"pyutils.verbose": {
					"type": "boolean",
					"scope": "window",
					"default": "false",
					"description": "Shows callback arguments in output channel, or maybe make one short output channel for text msgs and other for objects"
				},
				"pyutils.callback": {
					"type": "string",
					"scope": "window",
					"default": ".pyutils/file-modified",
					"description": "on modified callback"
				},
				"pyutils.executable": {
					"type": "string",
					"scope": "window",
					"default": ".pyutils/run-program",
					"description": "cwd and execute"
				},
				"pyutils.build": {
					"type": "string",
					"scope": "window",
					"default": ".pyutils/build",
					"description": "project build"
				},
				"pyutils.debug": {
					"type": "string",
					"scope": "window",
					"default": ".pyutils/debug",
					"description": "project debug"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"@vscode/test-electron": "^2.1.5",
		"eslint": "^8.24.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"ts-loader": "^9.4.1",
		"typescript": "^4.8.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	}
}
