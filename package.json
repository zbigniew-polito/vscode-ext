{
	"name": "beamy-vs-ext-pyutils",
	"displayName": "PyUtils",
	"description": "PyUtils",
	"publisher": "beamy",
	"version": "0.0.1",
	"repository": {
		"url": "url.to.repository.tld"
	},
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"workspaceContains:**/.pyutils/deactivate-me",
		"workspaceContains:.pyutils/deactivate-me"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "beamy-container",
					"title": "PyUtils",
					"icon": "$(wrench)"
				}
			],
			"panel": [
				{
					"id": "beamy-panel",
					"title": "Panel Test",
					"icon": "$(wrench)"
				}
			]
		},
		"customEditors": [
			{
			  "viewType": "pyutils.polyglot",
			  "displayName": "polyglot",
			  "selector": [
				{
				  "filenamePattern": "*.pg"
				}
			  ],
			  "priority": "default"
			},
			{
				"viewType": "pyutils.polyglot",
				"displayName": "polyglot",
				"selector": [
				  {
					"filenamePattern": "*.pg"
				  }
				],
				"priority": "default"
			},
			{
				"viewType": "pyutils.polyglot",
				"displayName": "polyglot",
				"selector": [
				  {
					"filenamePattern": "*.pg"
				  }
				],
				"priority": "default"
			},
			{
				"viewType": "pyutils.polyglot",
				"displayName": "polyglot",
				"selector": [
				  {
					"filenamePattern": "*.pg"
				  }
				],
				"priority": "default"
			  },
			  {
				"viewType": "pyutils.polyglot",
				"displayName": "polyglot",
				"selector": [
				  {
					"filenamePattern": "*.pg"
				  }
				],
				"priority": "default"
			  }.
			  ,


		],
		"views": {
			"beamy-container": [
				{
					"id": "polyglot",
					"name": "Polyglot",
					"icon": "media/dep.svg",
					"contextualTitle": "Polyglot"
				},
				{
					"id": "translator",
					"name": "Translator",
					"icon": "media/dep.svg",
					"contextualTitle": "Translator"
				},
				{
					"id": "notes",
					"name": "Notes",
					"icon": "$(pencil)",
					"contextualTitle": "Notes"
				},
				{
					"id": "clipboard",
					"name": "Clipboard",
					"icon": "$(scissors)",
					"contextualTitle": "Clipboard"
				},
				{
					"id": "scratchpad",
					"name": "Scratchpad",
					"icon": "$(pencil)",
					"contextualTitle": "Notes"
				},
				{
					"id": "locakdoc",
					"name": "Local Documentation",
					"icon": "media/dep.svg",
					"contextualTitle": "Local Documentation"
				},
				{
					"id": "unicode",
					"name": "Unicode Browser",
					"icon": "$(symbol-array)",
					"contextualTitle": "unicode"
				},
				{
					"id": "system",
					"name": "system",
					"icon": "$(paintcan)",
					"contextualTitle": "systemd"
				},
				{
					"id": "Utils",
					"name": "Utils",
					"icon": "$(vm)",
					"contextualTitle": "Utils"
				},
				{
					"id": "noise",
					"name": "noise",
					"icon": "$(notebook-render-output)",
					"contextualTitle": "noisesynth"
				},
				{
					"id": "drawit",
					"name": "drawing app",
					"icon": "$(paintcan)",
					"contextualTitle": "svgdraw"
				},
				{
					"id": "linguist",
					"name": "Linguist",
					"icon": "media/dep.svg",
					"contextualTitle": "Linguist"
				},
				{
					"id": "wave",
					"name": "Wave",
					"icon": "media/dep.svg",
					"contextualTitle": "Wave"
				}
			]
		},
		"themes": [
			{
				"label": "Viradora",
				"uiTheme": "vs-dark",
				"path": "./themes/viradora-color-theme.json"
			}
		],
		"colors": [
			{
				"id": "superstatus.error",
				"description": "Color for error message in the status bar.",
				"defaults": {
					"dark": "errorForeground",
					"light": "errorForeground",
					"highContrast": "#010203",
					"highContrastLight": "#feedc3"
				}
			}
		],
		"commands": [
			{
				"command": "pyutils.enable",
				"title": "Enable VsCodeUtil",
				"category": "ext"
			},
			{
				"command": "pyutils.disable",
				"title": "Disable VsCodeUtil",
				"category": "ext"
			},
			{
				"command": "pyutils.run",
				"title": "Run",
				"category": "ext",
				"icon": "$(play)"
			},
			{
				"command": "pyutils.first_in_context_menu_editor_title_in_tab",
				"title": "Some Context Menu Option",
				"category": "ext"
			},
			{
				"command": "pyutils.debug",
				"title": "Debug",
				"category": "ext",
				"icon": "$(debug)"
			}
		],
		"menus": {
			"editor/title/run": [
				{
					"command": "pyutils.run",
					"group": "navigation"
				}
			],
			"editor/title/context": [
				{
					"command": "pyutils.first_in_context_menu_editor_title_in_tab",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"command": "pyutils.debug",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"title": "PyUtils",
			"properties": {
				"pyutils.verbose": {
					"type": "boolean",
					"scope": "window",
					"default": "false",
					"description": "Shows callback arguments in output channel, or maybe make one short output channel for text msgs and other for objects"
				},
				"pyutils.callback": {
					"type": "string",
					"scope": "window",
					"default": ".pyutils/file-modified",
					"description": "on modified callback"
				},
				"pyutils.executable": {
					"type": "string",
					"scope": "window",
					"default": ".pyutils/run-program",
					"description": "cwd and execute"
				},
				"pyutils.build": {
					"type": "string",
					"scope": "window",
					"default": ".pyutils/build",
					"description": "project build command"
				},
				"pyutils.debug": {
					"type": "string",
					"scope": "window",
					"default": ".pyutils/debug",
					"description": "project debug command"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"@vscode/test-electron": "^2.1.5",
		"eslint": "^8.24.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"ts-loader": "^9.4.1",
		"typescript": "^4.8.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	}
}
